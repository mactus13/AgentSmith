{
    "machine": {
        "gpu": 1,
        "cpu": 1000,
        "ram": 8000
    },
    "job": {
        "APIVersion": "V1beta1",
        "Spec": {
            "Deal": {
                "Concurrency": 1
            },
            "Docker": {
                "Entrypoint": null,
                "EnvironmentVariables": [
                    {{ if .Prompt }}"{{ subst "PROMPT=%s" .Prompt }}",{{ end }}
                ],
                "Image": "zorlin/ollama:llama3-8b-lilypad1",
                "WorkingDirectory": ""
        },
        "Engine": "Docker",
        "Network": {
            "Type": "None"
        },
        "Outputs": [
            {
                "Name": "outputs",
                "Path": "/outputs"
            }
        ],
        "PublisherSpec": {
            "Type": "IPFS"
        },
        "Resources": {
            "GPU": "1",
            "CPU": "1000",
            "Memory": "8000"
        },
        "Timeout": 1800,
        "Verifier": "Noop"
        "Wasm": {
            "EntryModule": {}
        }
    }
  }
}